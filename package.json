{
  "name": "redux-react-boilerplate-2018",
  "version": "1.0.0",
  "description": "Redux enabled boilerplate with React, ReactBootstrap, Babel and Webpack",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "copy-assets": "node copyStaticAssets.js",
    "copy-assets-to-app": "node copyAssetsToApp.js",
    "watch": "webpack --watch",
    "build": "webpack --config webpack.prod.js",
    "build-static": "npm run build-html && npm run build-css && npm run copy-assets",
    "build-html": "pug static-pages/pug --out static-pages/dist/html --pretty",
    "build-css": "sass static-pages/scss/main.scss:static-pages/dist/css/main.css",
    "build-html-watch": "pug --watch static-pages/pug --out static-pages/dist/html --pretty",
    "build-css-watch": "sass --watch static-pages/scss/main.scss:static-pages/dist/css/main.css"
  },
  "keywords": [],
  "author": "hydrocarbons",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.9",
    "dotenv": "^6.0.0",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "prop-types": "^15.6.0",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "shelljs": "^0.8.2",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-dotenv-plugin": "^2.0.2",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "chart.js": "^2.7.2",
    "chartjs-plugin-datalabels": "^0.3.0",
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "moment": "^2.22.2",
    "pug-cli": "^1.0.0-alpha6",
    "query-string": "^6.1.0",
    "react-chartjs-2": "^2.7.2",
    "sass": "^1.9.0"
  }
}
